{
  "name": "@anoopqb/component-library",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "A modern React component library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run type-check",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anoopqb/nextjs-multisite-boilerplate.git",
    "directory": "component-library"
  },
  "license": "MIT"
}